      <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="139.0" text="Fermet le guichet" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER" />
      <Button mnemonicParsing="false" onAction="#deconnexion" text="Déconnexion" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
